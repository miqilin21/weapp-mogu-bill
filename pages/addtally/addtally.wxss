.one {
  top: 0;
  left: 0;
  width: 100%;
  background-size: cover;
  position: fixed;
  z-index: 999;
  background-color: #ebf4f4;
  height: 90rpx;
  overflow: hidden;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-around;
  background: #3eb575;
}

.tallytitle {
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  padding-bottom: 15rpx;
}

.outcometitle {
  display: flex;
  align-items: center;
  justify-content: center;
  height: 45rpx;
  font-size: 35rpx;
  letter-spacing: 2rpx;
  border: 1rpx solid black;
  border-radius: 8rpx 0 0 8rpx;
  width: 150rpx;
}

.incometitle {
  display: flex;
  align-items: center;
  justify-content: center;
  height: 45rpx;
  font-size: 35rpx;
  letter-spacing: 2rpx;
  border: 1rpx solid black;
  border-radius: 0 8rpx 8rpx 0;
  border-left: none;
  width: 150rpx;
}

.two {
  top: 60rpx;
  position: absolute;
  width: 100%;
  margin-bottom: 550rpx;
  overflow: hidden;
}

.swiper {
  border-top: 1rpx solid #ccc;
  width: 100%;
  height: 500rpx;
  overflow: hidden;
}

.sort {
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
  align-items: center;
  overflow: hidden;
  width: 100%;
  height: 500rpx;
}

.for {
  width: 20%;
  height: 150rpx;
  margin: 50rpx 0 50rpx 0;
  display: flex;
  justify-content: center;
  align-items: center;
}

.sort_data_item {
  display: flex;
  flex-direction: column;
  padding-top: 30rpx;
  padding-bottom: 30rpx;
  box-sizing: border-box;
  font-size: 26rpx;
}

.sort_data_item .icon {
  flex: 5;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 90rpx;
  height: 90rpx;
}

.border-radius {
  border-radius: 45rpx;
  position: relative;
  text-align: center;
}

.sort_data_item .text {
  flex: 5;
  display: flex;
  align-items: center;
  justify-content: center;
}

.calculate {
  background-size: cover;
  position: fixed;
  bottom: 0;
  z-index: 999;
  display: flex;
  flex-direction: column;
  height: 550rpx;
  width: 100%;
  align-items: center;
  justify-content: space-between;
  background-color: #f1f3f3;
}

.remark {
  flex: 1.6;
  width: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: flex-start;
  padding-left: 20rpx;
  padding-right: 20rpx;
  box-sizing: border-box;
  flex-shrink: 0;
}

.textdesc {
  width: 80rpx;
  flex: none;
}

.textinput {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  flex: auto;
}

.displaydata {
  flex: 1.6;
  width: 100%;
  display: flex;
  flex-direction: row;
  border-top: 1rpx solid #c3c6c7;
  border-bottom: 1rpx solid #c3c6c7;
  padding: 20rpx;
  justify-content: space-between;
  box-sizing: border-box;
  flex-shrink: 0;
}

.selectdata {
  display: flex;
  align-items: center;
  justify-content: space-between;
  height: 100%;
  flex: none;
  font-size: 30rpx;
}

.selecticon {
  width: 80rpx;
  border-radius: 50%;
  background: #3eb575;
  height: 80rpx;
  text-align: center;
  line-height: 80rpx;
}

.selecttext {
  padding-left: 10rpx;
  height: 80rpx;
  text-align: center;
  line-height: 80rpx;
  width: 110rpx;
}

.panel-display {
  display: flex;
  align-items: center;
  justify-content: flex-end;
  font-size: 40rpx;
  margin-left: 40rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  width: 100%;
  flex: auto;
}

.panel-btns {
  flex: 6.8;
  width: 100%;
  display: flex;
  flex-direction: column;
}

.btns-rows {
  width: 100%;
  flex: 1;
  display: flex;
  flex-direction: row;
  background-color: #f7f8f9;
}

.btn {
  text-align: center;
  box-sizing: border-box;
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
  border-top: 1rpx solid #c3c6c7;
}

.btn.active {
  background-color: #ebebeb;
}

.btn:not(:last-child) {
  border-right: 1rpx solid #c3c6c7;
}

.btns2-left-part {
  display: flex;
  flex-direction: row;
  flex-grow: 1;
}

.btn-c {
  color: #f00;
}

.btn-f {
  background: #927a30;
}

.iconfont1 {
  font-family: "iconfont";
  font-weight: normal;
  font-style: normal;
  font-size: 50rpx;
  line-height: 50rpx;
}
